{
  "$schema": "https://opencode.ai/config.json",

  /* **************************************************************************
  * MODEL
  * ************************************************************************ */
  "model": "github-copilot/claude-sonnet-4",
  "small_model": "github-copilot/gpt-4.1",

  /* **************************************************************************
  * PERMISSIONS
  * ************************************************************************ */
  "permission": {
    "edit": "allow",
    "webfetch": "allow",
    "bash": {
      "git push": "ask",
      "npm install": "ask",
      "bun add": "ask",
      "bun install": "ask",
      "*": "allow"
    }
  },

  /* **************************************************************************
  * MODES
  * ************************************************************************ */

    "mode": {
      "build": {
        "tools": {
          "bash": true,
          "edit": true,
          "glob": true,
          "grep": true,
          "ls": false, // prefer glob/grep
          "multiedit": true,
          "read": true,
          "task": true,
          "todowrite": true,
          "webfetch": true,
          "websearch": true,
          "write": true
        }
      },
      "plan": {
        "tools": {
          "bash": false,
          "edit": false,
          "glob": true,
          "grep": true,
          "ls": false, // prefer glob/grep
          "multiedit": false,
          "read": true,
          "task": true,
          "todowrite": true,
          "webfetch": true,
          "websearch": true,
          "write": false
        }
      }
  },

  /* **************************************************************************
  * PROVIDERS
  * ************************************************************************ */
  "provider": {
    "chutes-custom": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Chutes",
      "options": {
        "baseURL": "https://llm.chutes.ai/v1"
      },
      "models": {
        "openai/gpt-oss-120b": {
          "name": "GPT OSS 120B"
        },
        "moonshotai/Kimi-K2-Instruct-0905": {
          // https://huggingface.co/moonshotai/Kimi-K2-Instruct-0905
          "name": "Kimi K2 (0905)",
          "options": {
            "temperature": 0.6
          }
        },
        "zai-org/GLM-4.5-FP8": {
          "name": "GLM 4.5",
          "options": {
            "temperature": 0.6,
            "top_p": 1.0
          }
        },
        "deepseek-ai/DeepSeek-V3.1:THINKING": {
          // https://api-docs.deepseek.com/quick_start/parameter_settings
          // https://docs.unsloth.ai/models/deepseek-v3.1-how-to-run-locally
          "name": "DeepSeek V3.1 (Reasoning)",
          "options": {
            "temperature": 0.6
          }
        },
        "Qwen/Qwen3-Coder-480B-A35B-Instruct-FP8": {
          // https://huggingface.co/Qwen/Qwen3-Coder-480B-A35B-Instruct-FP8
          // https://docs.unsloth.ai/models/qwen3-coder-how-to-run-locally
          "name": "Qwen3 Coder",
          "options": {
            "temperature": 0.7,
            "top_p": 0.8,
            "top_k": 20,
            "repetition_penalty": 1.05,
            "max_tokens": 65536
          }
        },
        "Qwen/Qwen3-235B-A22B-Thinking-2507": {
          // https://huggingface.co/Qwen/Qwen3-235B-A22B-Thinking-2507
          "name": "Qwen3 235B (Reasoning)",
          "options": {
            "temperature": 0.6,
            "top_p": 0.95,
            "top_k": 20,
            "min_p": 0,
            "max_tokens": 32768 // REVIEW: test
          }
        },
        "Qwen/Qwen3-Next-80B-A3B-Thinking": {
          // https://huggingface.co/Qwen/Qwen3-Next-80B-A3B-Thinking
          "name": "Qwen3 Next (Reasoning)",
          "options": {
            "temperature": 0.6,
            "top_p": 0.95,
            "top_k": 20,
            "min_p": 0,
            "max_tokens": 32768 // REVIEW: test
          }
        }
      }
    }
  },

  /* **************************************************************************
   * LSP
   * ************************************************************************ */
  //"lsp": {},

  /* **************************************************************************
  * UI/UX
  * ************************************************************************ */
  "theme": "system",

  /* **************************************************************************
  * BINDS
  * ************************************************************************ */
  "keybinds": {
    "leader": "ctrl+x",
    "session_child_cycle": "<leader>]",
    "session_child_cycle_reverse": "<leader>[",
    "session_compact": "none",
    "session_share": "none"
  },

  /* **************************************************************************
  * MCPs
  * ************************************************************************ */
  "mcp": {
    "MCP_DOCKER": {
      "type": "local",
      "command": [
        "docker",
        "mcp",
        "gateway",
        "run"
      ]
    },
    "sequentialthinking": {
      "type": "remote",
      "url": "https://remote.mcpservers.org/sequentialthinking/mcp"
    },
    "deepwiki": {
      "type": "remote",
      "url": "https://mcp.deepwiki.com/mcp"
    }
  },

  /* **************************************************************************
  * MISC
  * ************************************************************************ */
  "instructions": ["{env:HOME}/.config/agents/instructions.md"],
  "autoupdate": false,
  "share": "manual"
}
